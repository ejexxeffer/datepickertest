{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/recommended",
    // "standard-with-typescript",
    "plugin:vue/vue3-essential",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "vue/require-default-prop": "off",
    "vue/no-reserved-component-names": "off"
  },
  "ignorePatterns": [
    "*/workbox-*.js",
    "*/sw.*",
    "*/registerSW.js",
    "/workbox-*.js",
    "/sw.*",
    "/registerSW.js"
  ]
}
